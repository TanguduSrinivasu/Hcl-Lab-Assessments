<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 5.4//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-5.4.dtd">

<hibernate-mapping>
	<class name="com.model.Employee" table="empl">
		<id name="empNo" column="empnumber">
			<generator class="increment"></generator>
		</id>

		<property name="firstName" column="firstname" length="20"></property>
		<property name="lastName" column="lastname" length="20"></property>
	</class>

</hibernate-mapping>